{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.chicken": {
      "arrays": {
        "textures": {
          "Array.Xskins": [
            "Texture.default",
            "Texture.default2",
            "Texture.default3",
            "Texture.default4"
          ]
        }
      },
      "geometry": "geometry.default",
      "materials": [
        { "*": "Material.default" },
        { "leg*": "Material.legs" }
      ],
      "textures": [ "Array.Xskins[variable.Xvar]" ],
      "is_hurt_color": {
        "r": 1.0,
        "g": 1.0,
        "b": 1.0,
        "a": "q.is_alive ? 0.2 : 0"
      }
    }
  }
}
